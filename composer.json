{
    "name": "mxms/checklist",
    "version": "1.0.1",
    "description": "Allows you to add checklists to Nova resources.",
    "keywords": [
        "laravel",
        "nova",
        "checklists"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Mxms\\Checklist\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mxms\\Checklist\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
